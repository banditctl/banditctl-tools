{
  "template": "## 🚀 $TITLE\n\n$CHANGES\n\n👥 Contributors: $CONTRIBUTORS",
  "categories": [
    { "title": "✨ Features", "labels": ["feat", "feature"] },
    { "title": "🐛 Fixes", "labels": ["fix", "bug"] },
    { "title": "🧰 Maintenance", "labels": ["chore", "refactor", "ci"] },
    { "title": "📚 Docs", "labels": ["docs"] },
    { "title": "🧪 Tests", "labels": ["test"] }
  ],
  "excludeLabels": ["skip-changelog"],
  "sort": "mergedAt"
}
